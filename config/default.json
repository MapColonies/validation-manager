{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8081",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "paths": {
    "pvPath": "/home/3DModels"
  },
  "projections": {
    "sphere": "+proj=geocent +datum=WGS84 +units=m +no_defs +type=crs",
    "region": "+proj=longlat +datum=WGS84 +no_defs +type=crs"
  },
  "percentageLimit": 10,
  "lookupTables": {
    "url": "http://127.0.0.1:8080",
    "subUrl": "lookup-tables/lookupData"
  },
  "catalog": {
    "url": "http://127.0.0.1:8081",
    "subUrl": "metadata"
  }
}
